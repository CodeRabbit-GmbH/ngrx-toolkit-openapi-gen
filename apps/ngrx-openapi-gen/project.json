{
  "name": "ngrx-openapi-gen",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/ngrx-openapi-gen/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "platform": "node",
        "outputPath": "dist/apps/ngrx-openapi-gen",
        "format": ["cjs"],
        "bundle": false,
        "main": "apps/ngrx-openapi-gen/src/main.ts",
        "tsConfig": "apps/ngrx-openapi-gen/tsconfig.app.json",
        "assets": [
          "apps/ngrx-openapi-gen/src/assets",
          "apps/ngrx-openapi-gen/README.md"
        ],
        "generatePackageJson": true,
        "esbuildOptions": {
          "sourcemap": true,
          "outExtension": {
            ".js": ".js"
          },
          "banner": {
            "js": "#!/usr/bin/env node"
          }
        }
      },
      "configurations": {
        "development": {},
        "production": {
          "bundle": true,
          "minify": true,
          "esbuildOptions": {
            "sourcemap": false,
            "outExtension": {
              ".js": ".js"
            },
            "banner": {
              "js": "#!/usr/bin/env node"
            }
          }
        }
      }
    },
    "nx-release-publish": {
      "executor": "@nx/js:release-publish",
      "options": {
        "packageRoot": "dist/apps/ngrx-openapi-gen"
      }
    },
    "prune-lockfile": {
      "dependsOn": ["build"],
      "cache": true,
      "executor": "@nx/js:prune-lockfile",
      "outputs": [
        "{workspaceRoot}/dist/apps/ngrx-openapi-gen/package.json",
        "{workspaceRoot}/dist/apps/ngrx-openapi-gen/package-lock.json"
      ],
      "options": {
        "buildTarget": "build"
      }
    },
    "copy-workspace-modules": {
      "dependsOn": ["build"],
      "cache": true,
      "outputs": [
        "{workspaceRoot}/dist/apps/ngrx-openapi-gen/workspace_modules"
      ],
      "executor": "@nx/js:copy-workspace-modules",
      "options": {
        "buildTarget": "build"
      }
    },
    "prune": {
      "dependsOn": ["prune-lockfile", "copy-workspace-modules"],
      "executor": "nx:noop"
    },
    "serve": {
      "continuous": true,
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "dependsOn": ["build"],
      "options": {
        "buildTarget": "ngrx-openapi-gen:build",
        "runBuildTargetDependencies": false
      },
      "configurations": {
        "development": {
          "buildTarget": "ngrx-openapi-gen:build:development"
        },
        "production": {
          "buildTarget": "ngrx-openapi-gen:build:production"
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/ngrx-openapi-gen/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
