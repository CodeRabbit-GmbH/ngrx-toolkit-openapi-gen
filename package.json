{
  "name": "@coderabbit/ngrx-toolkit-openapi-gen",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "npx nx serve ngrx-openapi-gen-playground",
    "build": "npx nx build ngrx-openapi-gen",
    "generate": "npx nx build ngrx-openapi-gen && node dist/apps/ngrx-openapi-gen/apps/ngrx-openapi-gen/src/main.js -i swagger.json -o tmp",
    "generate:zod": "npx nx build ngrx-openapi-gen && node dist/apps/ngrx-openapi-gen/apps/ngrx-openapi-gen/src/main.js -i swagger.json -o tmp --zod",
    "test": "npx nx test ngrx-openapi-gen",
    "test:e2e": "npx playwright test",
    "playground:start": "npm run playground:generate && npx nx serve ngrx-openapi-gen-playground",
    "playground:build": "npx nx build ngrx-openapi-gen-playground",
    "playground:generate": "npx nx run ngrx-openapi-gen-playground:generate-stores",
    "lint": "npx nx lint ngrx-openapi-gen",
    "docs:start": "cd docs-site && npm install && npm start",
    "docs:build": "cd docs-site && npm install && npm run build",
    "docs:serve": "cd docs-site && npm run serve",
    "docs:deploy": "cd docs-site && npm install && npx docusaurus deploy",
    "release": "npx nx release",
    "release:dry-run": "npx nx release --dry-run"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/ngrx-toolkit": "^20.5.0",
    "@angular/common": "21.0.6",
    "@angular/compiler": "21.0.6",
    "@angular/core": "21.0.6",
    "@angular/forms": "21.0.6",
    "@angular/platform-browser": "21.0.6",
    "@angular/platform-browser-dynamic": "21.0.6",
    "@angular/router": "21.0.6",
    "@ngrx/signals": "^21.0.0",
    "axios": "^1.6.0",
    "change-case": "^5.4.4",
    "commander": "^12.1.0",
    "picocolors": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.8.0",
    "ts-morph": "^27.0.2",
    "yaml": "^2.4.2",
    "zone.js": "0.16.0"
  },
  "devDependencies": {
    "@angular-devkit/core": "21.0.4",
    "@angular-devkit/schematics": "21.0.4",
    "@angular/build": "21.0.4",
    "@angular/cli": "21.0.4",
    "@angular/compiler-cli": "21.0.6",
    "@angular/language-service": "21.0.6",
    "@eslint/js": "^9.8.0",
    "@nx/angular": "22.3.3",
    "@nx/esbuild": "22.3.3",
    "@nx/eslint": "22.3.3",
    "@nx/eslint-plugin": "22.3.3",
    "@nx/jest": "22.3.3",
    "@nx/js": "22.3.3",
    "@nx/node": "22.3.3",
    "@nx/web": "22.3.3",
    "@nx/webpack": "22.3.3",
    "@nx/workspace": "22.3.3",
    "@playwright/test": "^1.57.0",
    "@schematics/angular": "21.0.4",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/jest": "30.0.0",
    "@types/node": "20.19.9",
    "@typescript-eslint/utils": "^8.40.0",
    "angular-eslint": "21.1.0",
    "autoprefixer": "^10.4.23",
    "daisyui": "^5.5.14",
